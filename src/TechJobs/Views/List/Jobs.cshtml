<h1>@ViewBag.title</h1>

<h3>@ViewBag.jobs.Count Result(s)</h3>


    @foreach (Dictionary<string, string> entry in ViewBag.jobs)
    {
        <table class="job-listing">
            @foreach (KeyValuePair<string, string> job in entry)
            {

                <tr>
                    <th>@job.Key</th>
                    <th>@job.Value</th>
                </tr>


            }


        </table>

    }



